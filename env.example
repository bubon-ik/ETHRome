# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
# REQUIRED for wallet connections
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID= 7b6f6c4eb808beb7bd577b581f1688f1

# 1inch API Key (get from https://portal.1inch.dev/)
# OPTIONAL - app works without API key (with rate limits)
# Required only for limit orders functionality
NEXT_PUBLIC_ONEINCH_API_KEY=your_1inch_api_key

# Base RPC URL (optional, defaults to public RPC)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Analytics (optional)
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
